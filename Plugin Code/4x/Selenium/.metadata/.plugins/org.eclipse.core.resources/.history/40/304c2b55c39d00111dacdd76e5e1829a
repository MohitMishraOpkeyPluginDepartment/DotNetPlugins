			public String Method_clickDropDownInTableCell(String object,String rowId,String colId,String type,String indx)
			{
			        		
						 boolean opvar;  
						 String mess,ret_mess;
						 Method_ObjectExistenceDynamicWait(object,timeoutselenium);
						 object=XmlString(object);
							try
							{
								//System.out.println("Method_ObjectClick"+object);
								object=object +"/tbody/tr["+rowId+"]/td["+colId+"]/"+type+"["+indx+"]";

								selenium.click(object);
								//setmessage("Object is Double Clicked");
								opvar=true;
								mess="Click on dropDown in table cell";
								
								 ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
							}
							 catch (Exception e)
							 {
						      //  setmessage(e.getMessage());
						        opvar=false;
						        mess=e.getMessage();
						         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
								  // e.printStackTrace();
						    }
							return ret_mess;
						
			}

public String Method_timeCounter()
{
        		
			 boolean opvar;  
			 String mess,ret_mess,opvars;
			 //Method_ObjectExistenceDynamicWait(object,timeoutselenium);
			// object=XmlString(object);
			 //Method_waitElementpresent(object);
				try
				{
					 Calendar now = Calendar.getInstance();
					   //System.out.println("Time Zone"+ now.getTimeZone());
					   
					    System.out.println("Current time : " + now.get(Calendar.HOUR_OF_DAY)
					                      + ":"
					                      + now.get(Calendar.MINUTE)
					                      + ":"
					                      + now.get(Calendar.SECOND));
					    opvars=""+now.get(Calendar.HOUR_OF_DAY) + ":"  + now.get(Calendar.MINUTE)+ ":" + now.get(Calendar.SECOND)+ ":" + now.get(Calendar.MILLISECOND);
			                     
			                    
			                      
			                     
					
					opvar=true;
					mess="Current time is fetched";
					 ret_mess="<KeywordOutput><Result><![CDATA["+opvars+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
					// ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
				}
				 catch (Exception e)
				 {
			       
			        opvar=false;
			        mess=e.getMessage();
			        ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
			        // ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
					  // e.printStackTrace();
			    }
				return ret_mess;
			
}
public String Method_verifyTextInPage(String expectedText)
{
 boolean opvar;
 String mess,ret_mess;
// Object=XmlString(Object);
try
{

opvar=selenium.isTextPresent(expectedText);
if(opvar==true){
	opvar=true;
	mess="text is verified";
	System.out.println(mess);
}else{
	opvar=false;
	mess="text is not verified";
	System.out.println(mess);
}
//setmessage("text is Clear");

 ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";

}
 catch (Exception e)
 {
   // setmessage(e.getMessage());
    opvar=false;
    mess=e.getMessage();
     ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
}
return ret_mess;
}
public String Method_verifyTextareaColsRowLength(String Object,String Row, String Col)
{        		
			 boolean opvar,x;  
			 String mess,ret_mess,opvars,opvars1;
			 Method_ObjectExistenceDynamicWait(Object,timeoutselenium);
			 Object=XmlString(Object);
				try
				{
					String object=Object+"@cols";
					opvars=selenium.getAttribute(object);	
					String object1=Object+"@rows";
					opvars1=selenium.getAttribute(object1);
					System.out.println("hi!!! this is opvars"+opvars+"  "+opvars1);
					if((opvars1.equalsIgnoreCase(Row)) && (opvars.equalsIgnoreCase(Col))){
						opvar=true;
						mess="TextArea length is verified";
						System.out.println(mess);
					}else{
						opvar=false;
						mess="TextArea length is not verified";
						System.out.println(mess);
					}
					
					//setmessage("Object is Double Clicked");
					
					 ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
				}
				 catch (Exception e)
				 {
			        //setmessage(e.getMessage());
			        opvar=false;
			        mess=e.getMessage();
			         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
					  // e.printStackTrace();
			    }
				return ret_mess;			
}

