public String Method_verifyTextInChildTableCell(String object,String propertyName,String propertyValue,String Rolid,String Colid, String expectedValue)
{
		
	 boolean opvar;  
	 String mess,ret_mess,result;
	 String execstring;
	 Method_ObjectExistenceDynamicWait(object,timeoutselenium);
	object=XmlString(object);
		try
		{
			//System.out.println("Method_ObjectClick"+object);
			mess="object is not found";
	//		"//input[contains(@"+propertyName+",'"+propertyValue+"')]"
			  if (propertyName.equalsIgnoreCase("text")|| propertyName.equalsIgnoreCase("innertext")) 	
				  execstring = "table[contains(text(),\'"+propertyValue+"\')]";
	
			  else
				  execstring= "table[@"+propertyName+"='"+propertyValue+"']";
			  object=object+"/descendant::"+execstring+"/tbody/tr["+Rolid+"]/td["+Colid+"]";					//div[contains(@class, 'x-combo-list')]/descendant::div[contains(@class, 'x-combo-list-item')][text()='AUTO']"
			  result=selenium.getText(object);
			  if(result.equalsIgnoreCase(expectedValue)){
				  mess="Text is verified"; 
					opvar=true;
			  }else{
				  mess="Text is not verified"; 
					opvar=false;
			  }
			
			ret_mess="<KeywordOutput><Result><![CDATA["+result+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
			//ret_mess="<KeywordOutput><Result>"+result+"</Result><Status>"+opvar+"</Status><Message>"+mess+"</Message></KeywordOutput>";
		}
		 catch (Exception e)
		 {
	       // setmessage(e.getMessage());
	        opvar=false;
	        mess=e.getMessage();
	        
	        ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
			  // e.printStackTrace();
	    }
		return ret_mess;
	
}

public String Method_verifyChildTableItem(String object,String type,String propertyName,String propertyValue,String index,String expectedValue)
{
		
	 boolean opvar;  
	 String mess,ret_mess,result;
	 String execstring;
	 Number no;
	 Method_ObjectExistenceDynamicWait(object,timeoutselenium);
	object=XmlString(object);
	 try
		{
			
			  if (propertyName.equalsIgnoreCase("text")|| propertyName.equalsIgnoreCase("innertext")) 	
				  execstring = "[contains(text(),\'"+propertyValue+"\')]";
	
			  else
				  execstring= "[@"+propertyName+"='"+propertyValue+"']";
			  execstring=type+execstring;
			  object=object+"/"+execstring;
			  no=selenium.getXpathCount(object);
			  if(no.equals("0"))
				  mess="Object is not present"; 
			  else  
			  	{
				  mess="Object is present";
				  object=object+"["+index+"]";
			  	}
			  //div[contains(@class, 'x-combo-list')]/descendant::div[contains(@class, 'x-combo-list-item')][text()='AUTO']"
			result=selenium.getText(object);
			if(result.equalsIgnoreCase(expectedValue)){
				mess="Table child item is verified"; 
				opvar=true;
			}else{
				mess="Table child item is verified"; 
				opvar=false;
			}
			
			ret_mess="<KeywordOutput><Result><![CDATA["+result+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
			//ret_mess="<KeywordOutput><Result>"+object+"</Result><Status>"+opvar+"</Status><Message>"+mess+"</Message></KeywordOutput>";
		}
		 catch (Exception e)
		 {
	       // setmessage(e.getMessage());
	        opvar=false;
	        mess=e.getMessage();
	         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
			  // e.printStackTrace();
	    }
		return ret_mess;
	
}

