	public String Method_goBackAndWait(String Time)
	{
	 boolean opvar;
	 //String opvars[];
	 String mess,ret_mess;
	 //selenium.setTimeout(timeoutselenium);
	
	try
	{
		int time=Integer.parseInt(Time);
		selenium.goBack();
		waiting(time);
		//selenium.waitForPageToLoad("3000");
	//setmessage("Object text is fetched");
	opvar=true;
	mess="go back is done" ;
	 ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";

	}
	 catch (Exception e)
	 {
        setmessage(e.getMessage());
        opvar=false;
        mess=e.getMessage();
         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
  }
	return ret_mess;
	}


