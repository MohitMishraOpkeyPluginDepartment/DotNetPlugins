public String Method_verifyTableColumnHeader(String object,String expextedText)
{
        		
			 boolean opvar=true,objth,objtd;  
			 String mess="",result[],ret_mess,opvars="",newobj1;
			 String no,row,newobj;
			 int count = 0;
			 result =new String[100];
			String exptxt[]=new String[100];
			// object=XmlString(object);
				try
				{
					//System.out.println("Method_ObjectClick"+object);
					mess="object is not found";
					if(selenium.isElementPresent(object+"/tbody/tr[1]/th")){
						newobj=object+"/tbody/tr[1]/th";
					}else{
						newobj=object+"/tbody/tr[1]/td";
					}
					Number s=selenium.getXpathCount(newobj);
					
						//no=Method_GetColCount(object);
						//noint=Integer.parseInt(no);
					   exptxt=expextedText.split(";");
						for(int i=1; i<=exptxt.length; i++)
						{	     
							//System.out.println(exptxt[i]);
									newobj1=newobj+"["+i+"]";
									//System.out.println(newobj);
									
									
									 result[i]= selenium.getText(newobj1);
										opvars=opvars+";"+result[i];
									
						}	  
					 
						for(int i=0; i<exptxt.length; i++){
							for(int k=1; k<=s.intValue(); k++){
								 if(exptxt[i].equalsIgnoreCase(result[k]))
								 {
									count++;
									
									break;
								}
								
							}
							
						}	
						if(count==exptxt.length){
							opvar=true;
							mess="Data is matched";
						}	else{
							opvar=false;
							mess="Data is not matched";
						}	
						
					
							
					

					ret_mess="<KeywordOutput><Result><![CDATA["+opvars+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
					//ret_mess="<KeywordOutput><Result>"+noint+"</Result><Status>"+opvar+"</Status><Message>"+mess+"</Message></KeywordOutput>";
				}
				 catch (Exception e)
				 {
			       // setmessage(e.getMessage());
			        opvar=false;
			        mess=e.getMessage();
			         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
					  // e.printStackTrace();
			    }
				return ret_mess;
			
}

