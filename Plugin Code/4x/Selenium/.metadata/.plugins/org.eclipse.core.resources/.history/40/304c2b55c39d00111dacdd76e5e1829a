	public String Method_ObjectClick(String object)
	{
	        		
				 boolean opvar;  
				 String mess,ret_mess;
				 Method_ObjectExistenceDynamicWait(object,timeoutselenium);
				 object=XmlString(object);
				 //Method_waitElementpresent(object);
					try
					{
						//System.out.println("Method_ObjectClick"+object);
					
						selenium.click(object);
						setmessage("Object is Clicked");
						opvar=true;
						mess="Object is Clicked";
						 ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						// ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
					}
					 catch (Exception e)
					 {
				        setmessage(e.getMessage());
				        opvar=false;
				        mess=e.getMessage();
				        ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
				        // ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						  // e.printStackTrace();
				    }
					return ret_mess;
				
	}
		
	
	public String ExcelDatabase(String DatabaseString) 
	{
		boolean opvar;
		String mess,ret_mess;
		try{
			
			WritableWorkbook workbook = Workbook.createWorkbook(new File("c:/test.xls"));
	       	WritableSheet sheet = workbook.createSheet("First Sheet", 0);
	    
	//String	DatabaseString = "ACTUAL_GT#:#:#:#VPMN#:#:#:#DUMMY_GT#:#:#:#USER_ID#:#:#:#HOST_ID@:@:@:@919769906806064#:#:#:#40420#:#:#:#919769891937016#:#:#:##:#:#:#1@:@:@:@919818997085#:#:#:#40410#:#:#:#919818850599#:#:#:##:#:#:#0@:@:@:@919818987990599#:#:#:#40410#:#:#:#919818814821533#:#:#:##:#:#:#0@:@:@:@919769961592626#:#:#:#40420#:#:#:#919769860020425#:#:#:##:#:#:#0@:@:@:@919769981825901#:#:#:#40420#:#:#:#919769802024628#:#:#:##:#:#:#0";
	 String [] row_wisedata = DatabaseString.split("@:@:@:@");

	 int row = 0;

	 for(int y = 0;y<=row_wisedata.length - 1;y++ ){
		 int col = 0;
		String row_wisedatasplitted = row_wisedata[y];
		String [] col_wisedata = row_wisedatasplitted.split("#:#:#:#");
		for (int g = 0;g<=col_wisedata.length-1;g++){
			 Label label = new Label(col, row,col_wisedata[g] ); 
	     	 sheet.addCell(label); 
	     	 System.out.println(g);
	     	// workbook.write(); 
	         col = col+1;
			
		}
		row = row+1;
		}
	 workbook.write(); 
		workbook.close();
		opvar=true;
		mess="Data is set";
		ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		}
		catch(Exception  e)
		{
			e.printStackTrace();
			opvar=false;
			mess=e.getMessage();
			ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		}
		return ret_mess;
	}

	
	
