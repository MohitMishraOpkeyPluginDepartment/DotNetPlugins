public String Method_verifyBrowserExist(String brwsr)
{
 boolean opvar;
 String opvars,mess,ret_mess,bw;		 
 //Object=XmlString(Object);
try	{						
	String htmlSource = selenium.getHtmlSource();			
	Pattern p = Pattern.compile("[t+T]*[i+I]*[t+T]*[l+L]*[e+E]>");
    String[] items = p.split(htmlSource);
    
    Pattern pp = Pattern.compile("</");
    String[] item = pp.split(items[1]);
    opvar=item[0].equalsIgnoreCase(brwsr);		    
    				
if(opvar==true)
	mess="Browser exist";
else{
	String br_nm[]=selenium.getAllWindowTitles();
	for(int i=0;i<br_nm.length;i++){
		if(brwsr.equalsIgnoreCase(br_nm[i])){
			opvar=true;
		}
		else
			opvar=false;
	}
	
}
if(opvar==true){
	mess="Browser exist";
	
}
else{
	mess="Browser not exist";
   
}
ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
}
 catch (Exception e)
 {	       
   opvar=false;
   mess=e.getMessage();
   ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
}		
 return ret_mess;
}
