	}
	
	public String Method_ObjectExistenceDynamicWait(String Object,String time)
	{
	 boolean opvar;
	 String opvars;
	 String mess,ret_mess;
	 Object=XmlString(Object);
	// Method_waitElementpresent(Object);
	try
	{	
		int time1 = Integer.parseInt(time);
		opvar=false;
		while(time1>0)
		{
			if(selenium.isElementPresent(Object))
				{
				opvar=true;
				break;
				}
			else

				waiting(1);
			time1=time1-1;
		}
		
		
		if(opvar!=true)
		{
			mess="element is not present ";
			
		}
		else
		{
			opvar=true;
			mess="element is present ";
		}
	
		ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA[True]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		//ret_mess="<KeywordOutput><Result>"+opvar+"</Result><Status>True</Status><Message>"+mess+"</Message></KeywordOutput>";
	}
	 catch (Exception e)
	 {
        setmessage(e.getMessage());
        opvar=false;
        mess=e.getMessage();
         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
  }
	return ret_mess;
	}
	
	
	public String Method_SelectMultipleItemList(String Object,String strvalue)
	{
	 boolean opvar;
	 String opvars;
	 String opvarsa[];
	 String mess,ret_mess;
	 Method_ObjectExistenceDynamicWait(Object,timeoutselenium);
	 Object=XmlString(Object);
	 //Method_waitElementpresent(Object);
	 //selenium.setTimeout(timeoutselenium);
	try
	{
		opvarsa=strvalue.split(";");
		selenium.removeAllSelections(Object);
		for(int i=0;i<opvarsa.length;i++)
			selenium.addSelection(Object, opvarsa[i]);
	//setmessage("Object text is fetched");
	opvar=true;
	mess="multple item is selected";
	 ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";

	}
	 catch (Exception e)
	 {
        setmessage(e.getMessage());
        opvar=false;
        mess=e.getMessage();
         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
  }
	return ret_mess;
	}

