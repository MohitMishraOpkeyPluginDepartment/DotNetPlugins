	 public String Method_typeKeysAndWait(String newproperty,String value,String time)
		{
		 boolean opvar;
		 String mess,ret_mess;
		 newproperty=XmlString(newproperty);
		 Method_waitElementpresent(newproperty);
		 //selenium.setTimeout(timeoutselenium);
		try
		{
		 
		selenium.typeKeys(newproperty, value);

		int time1=Integer.parseInt(time);
		waiting(time1);
		setmessage("text is set");
		opvar=true;
		mess="Text is Set";
		 ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
	
		}
		 catch (Exception e)
		 {
	        setmessage(e.getMessage());
	        opvar=false;
