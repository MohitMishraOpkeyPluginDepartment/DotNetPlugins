}public String Method_verifyEditBoxExist(String Object,String time)
{
	 boolean opvar=false,b;
	 String opvars;
	 
	 String mess="",ret_mess;
	 Method_ObjectExistenceDynamicWait(Object,timeoutselenium);
	 
	 Object=XmlString(Object);
	try
	{
		int time1=Integer.parseInt(time);
		for(int i=0; i<time1; i++){
				b=selenium.isElementPresent(Object);
				
			
			if(b==true){
				opvar=true;
				mess="Edit Box Exist is verified";
				break;
			}
			else
			{
					opvar =false;	
					mess="Edit Box Exist is not verified";
					
			}
			waiting(time1);
		}
	//mess="Object is Exist";
	 ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA[true]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";

	}
	 catch (Exception e)
	 {
       //setmessage(e.getMessage());
		 System.out.println(e.getMessage());
       opvar=false;
       mess=e.getMessage();
        ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
 }
	return ret_mess;
	}

public String Method_waitForEditBoxEnabled(String Object,String time)
{
 boolean opvar = false;
 String opvars;
 boolean x;
 
 String mess = null,ret_mess;
 Object=XmlString(Object);
 Method_waitElementpresent(Object);
try
{
int time1=Integer.parseInt(time);	
for(int i = 0 ;i<=time1;i++){

	x=selenium.isEditable(Object);
	if (x==true)
	{
		opvar=true;
		mess = "EditBox is Enabled";
		System.out.println(""+mess);
		break;
		
	}
	else
	{
		opvar =false;
		mess="EditBox is disabled";
		
	}

	waiting(1);
}
	

//mess="Object is focus";
 ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";

}
 catch (Exception e)
 {
    //setmessage(e.getMessage());
	 System.out.println(e.getMessage());
    opvar=false;
    mess=e.getMessage();
     ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
}
return ret_mess;
}

