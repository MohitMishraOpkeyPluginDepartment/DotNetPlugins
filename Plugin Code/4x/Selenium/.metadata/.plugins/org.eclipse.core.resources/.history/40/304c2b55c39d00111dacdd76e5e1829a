}public String Method_selectCheckBoxInTableCell(String object,String rowId,String colId,String type)
{
	
	 boolean opvar;  
	 String mess,ret_mess;
	 //object=XmlString(object);
		try
		{
			//System.out.println("Method_ObjectClick"+object);
			if(selenium.isElementPresent(object)){
				
			
			object=object +"/tbody/tr["+rowId+"]/td["+colId+"]/"+type;

			selenium.check(object);
			opvar=true;
			mess=" CheckBox is selected";
			}else{
				opvar=false;
				//mess="CheckBox is selected";
				mess="Object not found";
			}
			//setmessage("Object is Double Clicked");
			
			
			 ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		}
