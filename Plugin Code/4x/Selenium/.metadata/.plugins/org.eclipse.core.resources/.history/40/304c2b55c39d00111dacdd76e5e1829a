public String Method_verifyMultipleListItemExists(String Object,String strvalue)
{
boolean opvar;
String opvars[],opvarsa[];
String mess,ret_mess;
int count=0;
Object=XmlString(Object);
try
{

	opvarsa=strvalue.split(";");
	
	opvars=selenium.getSelectOptions(Object);
	
	for(int i=0;i<opvarsa.length;i++){
		for(int j=0;j<opvars.length;j++){
			if(opvarsa[i].equalsIgnoreCase(opvars[j])){
				count++;
			}				
		}				
	}
	if(count==opvarsa.length){
		opvar=true;
		mess="Drop Down items are verified";
	}
	else{
		opvar=false;
		mess="Drop Down items are not verified";
	}	
 ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA[true]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";

}
catch (Exception e)
{
  opvar=false;
  mess=e.getMessage();
  ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
}
return ret_mess;
}

public String Method_verifyAllListItemExists(String Object,String strvalue)
{
boolean opvar;
String opvarsa[],u_value[];
String mess,ret_mess;
int found=0;
Object=XmlString(Object);
try
{
	opvarsa=selenium.getSelectOptions(Object);		
	u_value=strvalue.split(";");
	
	for(int i=0;i<u_value.length;i++){
		for(int j=0;j<opvarsa.length;j++){
			if(u_value[i].equalsIgnoreCase(opvarsa[j])){
				found++;
			}				
		}				
	}
	if(found==opvarsa.length){
		opvar=true;
		mess="All drop down  items are verified";
	}
	else{
		opvar=false;
		mess="All drop down  items are not verified";
	}	 
  ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";

}
catch (Exception e)
{
  opvar=false;
  mess=e.getMessage();
  ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
}
return ret_mess;
}

