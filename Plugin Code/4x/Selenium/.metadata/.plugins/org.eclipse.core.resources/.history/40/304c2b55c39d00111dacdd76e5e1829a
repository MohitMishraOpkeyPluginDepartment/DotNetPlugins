public String Method_getTableColumnNumer(String object,String rowid,String text)
{
        		
			 boolean opvar;  
			 String mess,ret_mess;
			 String no,row,result,newobj;
			 int noint = 0;
			 Method_ObjectExistenceDynamicWait(object,timeoutselenium);
			object=XmlString(object);
				try
				{
					//System.out.println("Method_ObjectClick"+object);
					mess="object is not found";
					if(selenium.isElementPresent(object))
					{
						//no=Method_GetColCount(object);
						//noint=Integer.parseInt(no);
						for(int i=1;i<=500;i++)
						{	     
									newobj=object+"/tbody/tr["+rowid+"]/td["+i+"]";
									System.out.println(newobj);
									if(selenium.isElementPresent(newobj))
									{
										result= selenium.getText(newobj);
										System.out.println(result);
										if(result.equalsIgnoreCase(text))
										{
											noint=i;
											mess="Data is matched";
											break;
										}
										else
											mess="Data is not matched";
									}
									else
									{
										break;
									}
						}	  
						opvar=true;
						
							
					}
					else
					{
						mess="object is not found";
						opvar=false;
					}
					ret_mess="<KeywordOutput><Result><![CDATA["+noint+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
					//ret_mess="<KeywordOutput><Result>"+noint+"</Result><Status>"+opvar+"</Status><Message>"+mess+"</Message></KeywordOutput>";
				}
				 catch (Exception e)
				 {
			       // setmessage(e.getMessage());
			        opvar=false;
			        mess=e.getMessage();
			         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
					  // e.printStackTrace();
			    }
				return ret_mess;
			
}		

public String Method_clickButtonInTableCell(String object,String rowId,String colId)
{
		
	 boolean opvar;  
	 String mess,ret_mess;
	 Method_ObjectExistenceDynamicWait(object,timeoutselenium);
	object=XmlString(object);
