	 public String Method_verifyAllLink(String expectedLink)
		{
			boolean opvar;
			String opvars[],opvarst;
			String mess,ret_mess;
			int count=0;
			//Method_ObjectExistenceDynamicWait(Object,timeoutselenium);
			// Object=XmlString(Object);
			try
			{
			 
			 opvars=selenium.getAllLinks();
			//setmessage("Item is selected");
			opvar=true;
			mess="All Links are fetched";
			opvarst=opvars[0];
			
				
			String explnk[]=expectedLink.split(";");
			
			for(int i=0;i<explnk.length;i++){
				
				
					for(int j=0;j<opvars.length;j++)
					{
						if(explnk[i].equalsIgnoreCase(opvars[j])){
							
							
							count++;
							break;
							
						}
						opvarst=opvarst+";"+opvars[j];
					}
				
			}
			if(count==explnk.length){
				opvar=true;
				mess="All links are verified";
			}else{
				opvar=false;
				mess="All links are not verified";
			}
				
			ret_mess="<KeywordOutput><Result><![CDATA["+opvarst+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		    //ret_mess="<KeywordOutput><Result>"+opvarst+"</Result><Status>"+opvar+"</Status><Message>"+mess+"</Message></KeywordOutput>";
			}
			 catch (Exception e)
			 {
		        //setmessage(e.getMessage());
		        opvar=false;
		        mess=e.getMessage();
		         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		  }
			return ret_mess;
		}
