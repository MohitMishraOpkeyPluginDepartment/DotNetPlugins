public String Method_verifyListItemCount(String Object,String Value)
{
boolean opvar;
String opvars[];
String mess,ret_mess;
int count;
Method_ObjectExistenceDynamicWait(Object,timeoutselenium);
Object=XmlString(Object);
try
{	
count=Integer.parseInt(Value);
opvars=selenium.getSelectOptions(Object);
if(count==opvars.length)
{
	opvar=true;
   mess="ListItemCount is Verified"+(opvars.length);	
}
else{
	   opvar=false;
  mess="ListItemCount is Not Verified"+(opvars.length);
}
//ret_mess="<KeywordOutput><Result>"+(opvars.length)+"</Result><Status>"+opvar+"</Status><Message>"+mess+"</Message></KeywordOutput>";
ret_mess="<KeywordOutput><Result><![CDATA["+opvars.length+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";		}
catch (Exception e)
{
  //setmessage(e.getMessage());
  opvar=false;
  mess=e.getMessage();
  ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
}	
return ret_mess;
}

