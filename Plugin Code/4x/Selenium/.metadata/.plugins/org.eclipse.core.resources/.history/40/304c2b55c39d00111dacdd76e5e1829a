	public void Method_waitElementpresent(String str)
	{
	        		
				 boolean opvar;  
				 String mess,ret_mess;
				 boolean present;
					try
					{
						long t0, t1;
						System.out.println("Method_waitElementpresent");
						int n=Integer.parseInt(timeoutselenium);
						System.out.println("timeoutselenium "+timeoutselenium);
						System.out.println("n "+n);
				        t0 =  System.currentTimeMillis();
						do
						{
							present=selenium.isElementPresent(str);
							if(present)
							{
								break;
							}
							else
								waiting(1);
							t1 =  System.currentTimeMillis();
							
						}
						while ((t1 - t0) < (n * 1000));
					//	opvar=true;
					//	mess="Window is Selected";		
						// ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						// ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
					}
					 catch (Exception e)
					 {
				      //  setmessage(e.getMessage());
				      //  opvar=false;
				        mess=e.getMessage();
				        System.out.println("getMessage "+e.getMessage());
				     //   ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
				        // ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						  // e.printStackTrace();
				    }
				
				
	}
	
