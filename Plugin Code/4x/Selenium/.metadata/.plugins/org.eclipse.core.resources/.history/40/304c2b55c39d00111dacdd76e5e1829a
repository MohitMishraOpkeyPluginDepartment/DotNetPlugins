public String Method_waitForEditBoxDisabled(String Object,String time)
{
 boolean opvar=false;
 String opvars;
 String mess="",ret_mess;
 Object=XmlString(Object);
 Method_waitElementpresent(Object);
try
{
	int time1=Integer.parseInt(time);
	
	for(int i=1;i<=time1;i++)
	{   
	opvar=selenium.isEditable(Object);	
	if(opvar==false)
	{
		mess="Editbox disabled";
		opvar=true;
		break;		
	}
	else 
	{
		mess="Editbox was not disabled till the time mentioned";
	}
	waiting(1);
	}
	ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
	
}
 catch (Exception e)
 {
