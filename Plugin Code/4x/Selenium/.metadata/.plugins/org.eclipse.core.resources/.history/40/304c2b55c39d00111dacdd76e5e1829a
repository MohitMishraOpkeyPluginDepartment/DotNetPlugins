public String Method_verifyObjectdoesnotExists(String Object)
{
boolean opvar,x;
String opvars;
String mess,ret_mess;
Method_ObjectExistenceDynamicWait(Object,timeoutselenium);
Object=XmlString(Object);
//selenium.setTimeout(timeoutselenium);
try
{

x=selenium.isElementPresent(Object);
if(x==false){
opvar=true;
mess="Object does not Exists";
}

else{
opvar=false;
mess="Object Exist";
}
