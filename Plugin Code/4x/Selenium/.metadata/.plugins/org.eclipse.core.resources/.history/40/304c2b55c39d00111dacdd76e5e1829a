	//@Before
	
	public String Method_WebBrowserOpen(String browser,String url)
	{
		String ret_mess,mess,brow,baseurl="",index,newbrow[],indexurl="";
		Boolean opvar;
		String new2[];
		try
		{

				brow="*iehta" ;
				if(browser.equalsIgnoreCase("INTERNET EXPLORER"))
					brow="*iehta" ;
				else if(browser.equalsIgnoreCase("MOZILLA FIREFOX"))
					brow="*chrome";
				else if(browser.equalsIgnoreCase("OPERA"))
					brow="*opera";
				else if(browser.equalsIgnoreCase("CHROME"))
					brow="*googlechrome";
				else if(browser.equalsIgnoreCase("SAFARI"))
					brow="*safari";
			
		//url=url.toLowerCase();
		if(url.startsWith("http:")|| url.startsWith("https:"))	
				url=url;
		else
			url="http://"+url;
		System.out.println("url"+url);
			new2=url.split("/");
			System.out.println("i is "+new2.length);
			for(int i=0;i<new2.length;i++)
			{
				
				if(i==0)
					baseurl=baseurl+new2[0];
				if(i==1)
					baseurl=baseurl+"//";
				if(i==2)
					baseurl=baseurl+new2[2]+"/";
				if(i>2)
				{
					if(new2.length-1==i)
						indexurl=indexurl+new2[i];
					else
						indexurl=indexurl+new2[i]+"/";
				}
				
			}		
			selenium = new DefaultSelenium(host,port,brow,baseurl);
			selenium.start();
			int time=Integer.parseInt(timeoutselenium);
			String timeoutselenium12=time+"000";
			System.out.println(timeoutselenium12);
			System.out.println(timeoutselenium12);
			selenium.setTimeout(timeoutselenium12);
			selenium.open(indexurl);
			waiting(2);
			selenium.windowMaximize();
			selenium.windowFocus();
		opvar=true;
		mess="Web Browser is Open";
		 ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		}
		catch(Exception e)
		{
	    mess=e.getMessage();
		opvar=false;
		 ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		// ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		}
		return ret_mess;
	}

	public static void waiting (int n)
{
        
        long t0, t1;

        t0 =  System.currentTimeMillis();

        do{
            t1 = System.currentTimeMillis();
        }
        while ((t1 - t0) < (n * 1000));
    }

