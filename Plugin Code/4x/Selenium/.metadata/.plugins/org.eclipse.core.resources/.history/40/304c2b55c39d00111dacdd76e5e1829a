		selenium = new DefaultSelenium("localhost",5555, "*chrome",te);
		selenium.start();
		selenium.open("/");
		waiting(8);
		opvars=selenium.getLocation();
		System.out.println("Fetched"+opvars);
		te1=te.split(opvars);
		System.out.println(te1.length);
		System.out.println("map"+opvars);
		if(te1.length==1)
		{
			if(te.contains(".com/"))
			{
				te2=te.split(".com/");
				if(te2.length>1)
				{
					te2[1]="/"+ te2[1];
				System.out.println("te2"+te2[1]);
				selenium.open(te2[1]);
				}
				
			}
		}
		if(te1.length>1)
			{
			System.out.println("re1"+te1[1]);
			selenium.open(te1[1]);
			}
	}
	public void Setup(String host1,int port1,String steptimeout)
	{
		host=host1;
		port=port1;
		timeoutselenium=steptimeout;
		
