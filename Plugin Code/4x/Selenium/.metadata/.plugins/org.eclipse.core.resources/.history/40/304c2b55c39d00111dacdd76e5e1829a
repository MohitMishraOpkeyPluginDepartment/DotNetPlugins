	public String Method_closeSelectedWindow(String str)
	{
	        		
				 boolean opvar;  
				 String mess,ret_mess;
					try
					{
						selenium.selectWindow(str);
						waiting(2);
						selenium.keyDownNative("18");
						selenium.keyPressNative("115");
						selenium.keyUpNative("18");
						
						opvar=true;
						mess="Window is closed";		
						 ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						// ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
					}
					 catch (Exception e)
					 {
				        setmessage(e.getMessage());
				        opvar=false;
				        mess=e.getMessage();
				        ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
				        // ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						  // e.printStackTrace();
				    }
					return ret_mess;
				
	}
		
	public String Method_excel_compare(String filepath1,String filepath2)
	{
		boolean opvar;
		String mess,output ="",ret_mess;
		File inputWorkbook = new File(filepath1);
		Workbook w;
		File inputWorkbooksec = new File(filepath2);
		Workbook sec;
		try{
			w = Workbook.getWorkbook(inputWorkbook);
			sec = Workbook.getWorkbook(inputWorkbooksec);
			Sheet sheet = w.getSheet(0);
			Sheet sheet2 = sec.getSheet(0);
			String fail="";
            for (int j = 0; j < sheet.getColumns(); j++) 
			{
            	 for (int i = 0; i < sheet.getRows(); i++)
                 {
     			
            		 Cell cell = sheet.getCell(j,i); 
            		 Cell cell2 = sheet2.getCell(j,i);
            		 String secdata = cell2.getContents();
            		 String firstdata=cell.getContents();
            		 System.out.println(firstdata);
            		 System.out.println(secdata);
            		 if(firstdata.equalsIgnoreCase(secdata))
            		 {
            	
            		 }
            		 else
            		 {
            			 fail=fail+"("+(j+1)+","+(i+1)+","+secdata+");";
            		 }
                 }
			}
            	 System.out.println(fail);
            opvar=true;
			mess="Two excel file is compared successfully";
			 ret_mess="<KeywordOutput><Result><![CDATA["+fail+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		}
		catch(Exception e)
		{
			
			opvar=false;
		  mess=e.getMessage();
		  ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		}
		return ret_mess;
	}
	 

	
	
