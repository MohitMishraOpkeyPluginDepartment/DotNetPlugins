	public String Method_getFullTableText(String object)
	{
	        		
				 boolean opvar;  
				 String mess,ret_mess;
				 String no,row,result ="",nocol,newobj,newresult="";
				 int noint = 0,colno=0;
				 Method_ObjectExistenceDynamicWait(object,timeoutselenium);
				 object=XmlString(object);
				 //Method_waitElementpresent(object);
					try
					{
						//System.out.println("Method_ObjectClick"+object);
						mess="object is not found";
						if(selenium.isElementPresent(object))
						{
							//no=Method_GetRowCount(object);
						//	nocol=Method_GetColCount(object);
							//noint=Integer.parseInt(no);
							//colno=Integer.parseInt(nocol);
							for(int i=1;i<=1000;i++)
							{	     
								newobj=object+"/tbody/tr["+i+"]";  
								if(selenium.isElementPresent(newobj))
								{
									for(int j=1;j<=500;j++)
									{
										//newobj=object+"/tbody/tr["+i+"]";
										
											newobj=object+"/tbody/tr["+i+"]/td["+j+"]";
											System.out.println(newobj);
											if(selenium.isElementPresent(newobj))
											{
												result=selenium.getText(newobj);
												//System.out.println(result);
												if(result.isEmpty()==false)	
													newresult=newresult+result+";";
												System.out.println(newresult);
											}
											else
											{
												break;
											}
									}		
									
								}		
								else
								{
								 break;
								}
								
							}  
							opvar=true;
							mess="data is retrive";
						}
						else
						{
							mess="object is not found";
							opvar=false;
						}
						ret_mess="<KeywordOutput><Result><![CDATA["+newresult+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						//ret_mess="<KeywordOutput><Result>"+newresult+"</Result><Status>"+opvar+"</Status><Message>"+mess+"</Message></KeywordOutput>";
					}
					 catch (Exception e)
					 {
				        setmessage(e.getMessage());
				        opvar=false;
				        mess=e.getMessage();
				         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";

				    }
					return ret_mess;
				
	}
	
	public String Method_VerifyUITableDataWithExcel(String Excelsheetpath1,String filepath)
	{
	        		
				 boolean opvar;  
				 String mess,ret_mess;
				 String elemValue,result ="",nocol,newobj,newresult="",output = "";
				 int index=0;
					try
					{
							FileInputStream fstream=new FileInputStream(filepath);
							DataInputStream in=new DataInputStream(fstream);
							BufferedReader br=new BufferedReader(new InputStreamReader(in));
							String str,newstr="",match="";
							while((str=br.readLine())!= null)
							{
								newstr=newstr+str+"\n";
							}
							in.close();
							File excelsheetpath=new File(Excelsheetpath1);
							Workbook sec;
							sec=Workbook.getWorkbook(excelsheetpath);
							Sheet sheet= sec.getSheet(0);
							FileWriter fstream1=new FileWriter("c:/out.txt");
							BufferedWriter out=new BufferedWriter(fstream1);
							String sour[],sour1[],sour2[],sour3[],sour4[],sour5[];
							sour=newstr.split("Contract_Re_Sch_List_Block_tbody");
							sour1=sour[1].split("</table");
							sour2=sour1[0].split("</td>");
							int k=1,j=0;
							for(int i=1;i<sour2.length;i++)
							{
								try
								{
									if(j==22)
									{
										k=k+1;
										j=0;
										continue;
									}
										
									Cell cell=sheet.getCell(j,k);
									String Firstdata=cell.getContents();
									sour5=sour2[i].split("<td");
									sour3=sour5[1].split("<input type=");
									sour4=sour3[0].split(">");
									sour4[1]=sour4[1].replace("\n","");
									sour4[1]=sour4[1].replace(" ","");
									sour4[1]=sour4[1].replace("\t","");
									out.write(sour4[1]);
									j=j+1;
									if(Firstdata.equalsIgnoreCase(sour4[1]))
									{
										
									}
									else
									{
										index=index+1;
										opvar=false;
										output=output+"("+(k)+","+(j)+","+sour4[1]+")";
										
									}
									
								}
								catch(Exception e)
								{
									out.write(e.getMessage());
								}
							}
							out.close();
							if(index==0)
								opvar=true;
							else
								opvar=false;
							mess="data is verified";
							ret_mess="<KeywordOutput><Result><![CDATA["+output+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
					
					}
					
						
						//ret_mess="<KeywordOutput><Result>"+newresult+"</Result><Status>"+opvar+"</Status><Message>"+mess+"</Message></KeywordOutput>";
					
					 catch (Exception e)
					 {
				        setmessage(e.getMessage());
				        opvar=false;
				        mess=e.getMessage();
				         ret_mess="<KeywordOutput><Result><![CDATA["+output+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";

				    }
					return ret_mess;
				
	}	
	

