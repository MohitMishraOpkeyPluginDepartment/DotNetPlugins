	public String Method_GetItemList(String Object)
	{
	 boolean opvar;
	 String opvars[],opvarst;
	 String mess,ret_mess;
	 Method_ObjectExistenceDynamicWait(Object,timeoutselenium);
	 Object=XmlString(Object);
	// Method_waitElementpresent(Object);
	 //selenium.setTimeout(timeoutselenium);
	try
	{
	
		opvars=selenium.getSelectOptions(Object);
	//setmessage("Object text is fetched");
	opvar=true;
	mess="All item be fetched";
	opvarst=opvars[0];
	for(int i=1;i<opvars.length;i++)
		opvarst=opvarst+";"+opvars[i];
	ret_mess="<KeywordOutput><Result><![CDATA["+opvarst+"]]?</Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
	//ret_mess="<KeywordOutput><Result>"+opvarst+"</Result><Status>"+opvar+"</Status><Message>"+mess+"</Message></KeywordOutput>";

	}
	 catch (Exception e)
	 {
        setmessage(e.getMessage());
        opvar=false;
        mess=e.getMessage();
         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
  }
	return ret_mess;
	}
	
	public String Method_verifyAllListItems(String Object,String ValueString)
	{
	 boolean opvar;
	 String opvars[],opvarst,userstr[];
	 String mess,ret_mess;
	 Method_ObjectExistenceDynamicWait(Object,timeoutselenium);
	 Object=XmlString(Object);
	 //Method_waitElementpresent(Object);
	 userstr=ValueString.split(";");
	 //selenium.setTimeout(timeoutselenium);
	try
	{
	
		opvars=selenium.getSelectOptions(Object);
	//setmessage("Object text is fetched");
	opvar=true;
	mess="All item be fetched";
	opvarst=opvars[0];
	int match=0;
	for(int j=0;j<userstr.length;j++)	
		for(int i=0;i<opvars.length;i++)
		{	if(userstr[j].equalsIgnoreCase(opvars[i]))
				{
					match=match+1;
					break;
				}
		}
	if(match==userstr.length)
		{
		opvar=true;
		mess="All item matched";
		}
	else
	{
		mess="item not matched";
		opvar=false;
	}
	ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA[True]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
	//ret_mess="<KeywordOutput><Result>"+opvar+"</Result><Status>True</Status><Message>"+mess+"</Message></KeywordOutput>";

	}
	 catch (Exception e)
	 {
        setmessage(e.getMessage());
        opvar=false;
        mess=e.getMessage();
         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
  }
	return ret_mess;
	}
	
	
