String opvarsa[],u_value[];
String mess,ret_mess;
int found=0;
Method_ObjectExistenceDynamicWait(Object,timeoutselenium);
Object=XmlString(Object);
try
{
	int time1=Integer.parseInt(time);
	//opvarsa=selenium.getSelectOptions(Object);		
	//u_value=strvalue.split(";");

	opvarsa=strvalue.split(";");
	selenium.removeAllSelections(Object);
	for(int i=0;i<opvarsa.length;i++){
		selenium.addSelection(Object, opvarsa[i]);
		waiting(time1);
	
	}
	
		opvar=true;
		mess="multple  item is selected";
	 
  ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";

}
catch (Exception e)
{
  opvar=false;
  mess=e.getMessage();
  ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
}
return ret_mess;
