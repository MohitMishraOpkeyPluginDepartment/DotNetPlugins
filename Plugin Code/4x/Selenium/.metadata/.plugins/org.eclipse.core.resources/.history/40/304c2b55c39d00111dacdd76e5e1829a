	public String Method_CheckOnOff(String newproperty,String value)
	{
		boolean opvar; 
		String ret_mess,mess;
		 newproperty=XmlString(newproperty);
		 Method_waitElementpresent(newproperty);
		 //selenium.setTimeout(timeoutselenium);
		try
		{
			if(value.equalsIgnoreCase("on"))
				{
				selenium.check(newproperty);
				mess="CheckBox is checked";
				}
			else
				{
				selenium.uncheck(newproperty);
				mess="CheckBox is unchecked";
				}
			opvar=true;
		      ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
			
		}
		 catch (Exception e)
		 {
	        setmessage(e.getMessage());
	        opvar=false;
	        mess=e.getMessage();
	         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
