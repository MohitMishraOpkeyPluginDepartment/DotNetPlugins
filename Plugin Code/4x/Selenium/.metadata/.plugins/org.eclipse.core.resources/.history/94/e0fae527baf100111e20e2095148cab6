	}
	catch(Exception e)
	{
		System.out.print(e);
		opvar = false;
		 ret_mess = "<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA[true]]></Status><Message><![CDATA[VAlue is fetch]]></Message></KeywordOutput>";
		  	
	}
	return ret_mess;
}
