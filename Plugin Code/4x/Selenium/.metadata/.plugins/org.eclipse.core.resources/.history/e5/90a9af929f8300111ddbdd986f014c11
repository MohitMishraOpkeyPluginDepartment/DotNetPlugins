			public static String Method_SetValueToExcelFile(String path,String sheetname,String columnname,String rowno,String data){
		    	boolean opvar = false;
		    	String ret_mess,mess;
		    	int col_locstr = 0;
		    	try{
		    		Workbook workbook = Workbook.getWorkbook(new File(path));
		    		WritableWorkbook copy = Workbook.createWorkbook(new File(path), workbook);
		     		WritableSheet sheet = copy.getSheet(sheetname);
		    		 int	col_count = sheet.getColumns();
		    	       for(int h = 0;h<=col_count;h++){
		    	    	   Cell a1 = sheet.getCell(h,0); 
		    	    	   String colval = a1.getContents();
		    	    	 boolean x =   colval.equalsIgnoreCase(columnname);
		    	    	
		    	    	   if(x == true){
		    	    		 int  col_loc = h;
		    	    		   col_locstr =col_loc;
		    	    		  h = col_count;
		    	    		
		    	    	   }
		    	       }
		    	       String aString = rowno;
		    	       int aInt = Integer.parseInt(aString);
		    	       aInt = aInt -1;
		    	       Label label = new Label(col_locstr, aInt,data ); 
		    	       sheet.addCell(label); 
		    	       copy.write(); 
		    			copy.close();
		    			opvar = true;
		    			mess = "value is set to excel sheet";
		    			ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		    					
		    		
		    	}catch(Exception e){
		    		opvar = false;
		    		mess = e.getMessage();
		    		ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		    		e.printStackTrace();
		    	}
		    	return ret_mess;
		    	
		    	
		    }

			public static String Method_CreateExcelWorkbook(){
		    	boolean opvar = false;
		    	String ret_mess,mess;
		    	int col_locstr = 0;
		    	try
		        {
		          String filename = "D:/nazim/abc.xls";
		          WorkbookSettings ws = new WorkbookSettings();
		          ws.setLocale(new Locale("en", "EN"));
		          WritableWorkbook workbook = 
		            Workbook.createWorkbook(new File(filename), ws);
		          WritableSheet s = workbook.createSheet("Sheet1", 0);
		          WritableSheet s1 = workbook.createSheet("Sheet1", 0);
		     
		          workbook.write();
		          workbook.close();     
		          opvar = true;
	    			mess = "Excel sheet is creted.";
	    			ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
	    					
		       
			}catch(Exception e){
	    		opvar = false;
	    		mess = e.getMessage();
	    		ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
	    		e.printStackTrace();
	    	}
           return ret_mess;
		    	
		    	
		    }	
			 
			public static String Method_DeleteAllDataRowsFromExcel(String path,String sheetname){
		    	boolean opvar = false;
		    	String ret_mess,mess;
		    	int col_locstr = 0;
		    	try{
		    		Workbook workbook = Workbook.getWorkbook(new File(path));
		    		WritableWorkbook copy = Workbook.createWorkbook(new File(path), workbook);
		     		WritableSheet sheet = copy.getSheet(sheetname);
		    		 int	col_count = sheet.getColumns();
		    		 int row_count = sheet.getRows();
		    	   
		    	    for(int j=1; j<=row_count; j++){
		    	    		  sheet.removeRow(2);
		    	    		     	    	  
		    	      }
		    	   // workbook.save("path");
		    	    
                  
		    	    
		    	      // Label label = new Label(col_locstr, aInt,data ); 
		    	      // sheet.addCell(label); 
		    	      // copy.write(); 
		    			//copy.close();
		    			opvar = true;
		    			mess = "All rows deleted";
		    			ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		    					
		    		
		    	}catch(Exception e){
		    		opvar = false;
		    		mess = e.getMessage();
		    		ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		    		e.printStackTrace();
		    	}
		    	return ret_mess;
		    	
		    	
		    }
			

			public static String Method_WriteInExcel(String path,String sheetname,String columnname,String data){
		    	boolean opvar = false;
		    	String ret_mess,mess;
		    	int col_locstr = 0;
		    	try{
		    		Workbook workbook = Workbook.getWorkbook(new File(path));
		    		WritableWorkbook copy = Workbook.createWorkbook(new File(path), workbook);
		     		WritableSheet sheet = copy.getSheet(sheetname);
		    		 int	col_count = sheet.getColumns();
		    		 int row_count=sheet.getRows();
		    	       for(int h = 0;h<=col_count;h++){
		    	    	   Cell a1 = sheet.getCell(h,0); 
		    	    	   String colval = a1.getContents();
		    	    	 boolean x =   colval.equalsIgnoreCase(columnname);
		    	    	
		    	    	   if(x == true){
		    	    		 int  col_loc = h;
		    	    		   col_locstr =col_loc;
		    	    		  h = col_count;
		    	    		
		    	    	   }
		    	       }
		    	       
		    	       
		    	      //row_count = row_count -1;
		    	       Label label = new Label(col_locstr,row_count ,data ); 
		    	       sheet.addCell(label); 
		    	       copy.write(); 
		    			copy.close();
		    			opvar = true;
		    			mess = "value is set to excel sheet";
		    			ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		    					
		    		
		    	}catch(Exception e){
		    		opvar = false;
		    		mess = e.getMessage();
		    		ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		    		e.printStackTrace();
		    	}
		    	return ret_mess;
		    	
		    	
		    }
			
