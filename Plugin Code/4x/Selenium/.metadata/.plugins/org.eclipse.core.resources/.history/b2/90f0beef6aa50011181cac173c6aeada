	public String Method_goForward()
	{
	        		
				 boolean opvar;  
				 String mess,ret_mess;
				 
					try
					{
						//System.out.println("Method_ObjectClick"+object);
						String script=" javascript:history.go(+1)";
						selenium.runScript(script);
						setmessage("Go Forward is done");
						opvar=true;
						mess="Go Forward is done";
						 ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						// ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
					}
					 catch (Exception e)
					 {
				        setmessage(e.getMessage());
				        opvar=false;
				        mess=e.getMessage();
				         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						  // e.printStackTrace();
				    }
					return ret_mess;
				
	}
	
	public String Method_excel_verify(String filepath,String colheader1,String colheader2,String colheader3,String data1,String data2)
	{
		boolean opvar;
		String mess,output ="",ret_mess;
		File inputWorkbook = new File(filepath);
		Workbook w;
		try{
			int colpos[] ={1,1,1};
			int k=0;
			String header[]={colheader1,colheader2,colheader3};
            String value[]={data1,data2};
			w = Workbook.getWorkbook(inputWorkbook);
			Sheet sheet = w.getSheet(0);
            for (int j = 0; j < sheet.getColumns(); j++) 
			{
            	Cell cell = sheet.getCell(j,0); 
            	 System.out.println(cell.getContents());
            	if(cell.getContents().equals(header[k]))
            	{
            		colpos[k]=j;
            		 System.out.println(colpos[k]);
            		k++;
            		if(k==3)
            			break;
            	}
			}
            System.out.println(colpos[2]);
            for (int i = 0; i < sheet.getRows(); i++)
            {
					Cell cell = sheet.getCell(colpos[0], i);
					System.out.println(cell.getContents());
					if(cell.getContents().equals(value[0]))
					{
						cell = sheet.getCell(colpos[1], i);
						if(cell.getContents().equals(value[1]))
						{
							cell = sheet.getCell(colpos[2], i);
							System.out.println(cell.getContents());
							output=cell.getContents();
							break;
						}
					}
								
            } 
			opvar=true;
			mess="data is get";
			 ret_mess="<KeywordOutput><Result><![CDATA["+output+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		}
		catch(Exception e)
		{
			
			opvar=false;
		  mess=e.getMessage();
		  ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		}
		return ret_mess;
	}
	 

	public String Method_goForwardAndWait()
	{
	        		
				 boolean opvar;  
				 String mess,ret_mess;
				 
					try
					{
						//System.out.println("Method_ObjectClick"+object);
						String script=" javascript:history.go(+1)";
						selenium.runScript(script);
						setmessage("Go Forward is done");
						opvar=true;
						mess="Go Forward is done";
						//waiting(5);
						selenium.waitForPageToLoad("3000");
						 ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
					}
					 catch (Exception e)
					 {
				        setmessage(e.getMessage());
				        opvar=false;
				        mess=e.getMessage();
				         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						  // e.printStackTrace();
				    }
					return ret_mess;
				
	}

	
