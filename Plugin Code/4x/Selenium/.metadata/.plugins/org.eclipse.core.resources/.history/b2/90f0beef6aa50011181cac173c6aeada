	}
		
	
	public String Method_keyDownNative(String data)
	{
		boolean opvar;
		String mess, ret_mess;
		try{
			 opvar=true;
		System.out.println("data is "+data);
		selenium.keyDownNative(data);
		mess="key down is pressed";
	 	ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		}
		catch(Exception e)
		{
			opvar=false;
		  mess=e.getMessage();
		  ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
			
		}
		return ret_mess;
	}
	
	public String Method_keyPressNative(String data)
	{
		boolean opvar;
		String mess,ret_mess;
		try{
			 opvar=true;
		
		System.out.println("data is "+data);
		data=data.toUpperCase();
		for(int i=0;i<data.length();i++)
		{
			
			char s=data.charAt(i);
			int j=s;
			
			if(s=='!')
				{
				selenium.keyDownNative("16");
				selenium.keyPressNative("49");
				selenium.keyUpNative("16");
				}
			else if(s==':')
				{
				System.out.println("hyhd");
				selenium.keyDownNative("16");
				selenium.keyPressNative("59");
				selenium.keyUpNative("16");
				}
			else if(s=='@')
			{
			
			selenium.keyDownNative("16");
			selenium.keyPressNative("50");
			selenium.keyUpNative("16");
			}	
			else if(s=='#')
			{
			System.out.println("hyhd");
			selenium.keyDownNative("16");
			selenium.keyPressNative("51");
			selenium.keyUpNative("16");
			}
			else if(s=='$')
			{
			System.out.println("hyhd");
			selenium.keyDownNative("16");
			selenium.keyPressNative("52");
			selenium.keyUpNative("16");
			}
			else if(s=='%')
			{
			System.out.println("hyhd");
			selenium.keyDownNative("16");
			selenium.keyPressNative("53");
			selenium.keyUpNative("16");
			}
			else if(s=='^')
			{
			System.out.println("hyhd");
			selenium.keyDownNative("16");
			selenium.keyPressNative("54");
			selenium.keyUpNative("16");
			}
			else if(s=='&')
			{
			System.out.println("hyhd");
			selenium.keyDownNative("16");
			selenium.keyPressNative("55");
			selenium.keyUpNative("16");
			}
			else if(s=='*')
			{
			System.out.println("hyhd");
			selenium.keyDownNative("16");
			selenium.keyPressNative("56");
			selenium.keyUpNative("16");
			}
			else if(s==')')
			{
			System.out.println("hyhd");
			selenium.keyDownNative("16");
			selenium.keyPressNative("48");
			selenium.keyUpNative("16");
			}	
			else if(s=='(')
			{
			System.out.println("hyhd");
			selenium.keyDownNative("16");
			selenium.keyPressNative("57");
			selenium.keyUpNative("16");
			}
			else if(s=='_')
			{
			System.out.println("hyhd");
			selenium.keyDownNative("16");
			selenium.keyPressNative("45");
			selenium.keyUpNative("16");
			}
			else if(s=='+')
			{
			System.out.println("hyhd");
			selenium.keyDownNative("16");
			selenium.keyPressNative("61");
			selenium.keyUpNative("16");
			}
			else if(s=='|')
			{
			System.out.println("hyhd");
			selenium.keyDownNative("16");
			selenium.keyPressNative("92");
			selenium.keyUpNative("16");
			}
			else if(s=='{')
			{
			System.out.println("hyhd");
			selenium.keyDownNative("16");
			selenium.keyPressNative("91");
			selenium.keyUpNative("16");
			}
			else if(s=='}')
			{
			System.out.println("hyhd");
			selenium.keyDownNative("16");
			selenium.keyPressNative("93");
			selenium.keyUpNative("16");
			}
			else if(s=='<')
			{
			System.out.println("hyhd");
			selenium.keyDownNative("16");
			selenium.keyPressNative("44");
			selenium.keyUpNative("16");
			}
			else if(s=='>')
			{
			System.out.println("hyhd");
			selenium.keyDownNative("16");
			selenium.keyPressNative("46");
			selenium.keyUpNative("16");
			}
			else if(s=='?')
			{
			System.out.println("hyhd");
			selenium.keyDownNative("16");
			selenium.keyPressNative("47");
			selenium.keyUpNative("16");
			}
			else
			{
			String a=""+j;
			System.out.println(a);
			selenium.keyPressNative(a);
			}
		}
	 mess="key down is pressed";
	 ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		}
		catch(Exception e)
		{
			
			opvar=false;
		  mess=e.getMessage();
		  ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
			
		}
		return ret_mess;
	}
	
