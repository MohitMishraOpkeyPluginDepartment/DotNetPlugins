	public String Method_ObjectTextVerification(String Object,String Text)
	{
	 boolean opvar;
	 String opvars;
	 String mess,ret_mess;
	 Object=XmlString(Object);
	try
	{
		opvars=selenium.getText(Object);
		if(opvars.equalsIgnoreCase(Text))
		{
			opvar=true;
			mess="Text is matched";
			
		}
		else
		{
			opvar=false;
			mess="Text is Not matched";
		}
	
		ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA[True]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		//ret_mess="<KeywordOutput><Result>"+opvar+"</Result><Status>True</Status><Message>"+mess+"</Message></KeywordOutput>";
	}
	 catch (Exception e)
	 {
        setmessage(e.getMessage());
        opvar=false;
        mess=e.getMessage();
         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
  }
	return ret_mess;
	}
	
	public String Method_VerifyPropertyValue(String Object,String PropertyValue,String expectedVal)
	{
	 boolean opvar;
	 String opvars;
	 String mess,ret_mess;
	 Object=XmlString(Object);
	try
	{	
		Object=Object+"@"+PropertyValue;
		if(PropertyValue.equalsIgnoreCase("innertext"))
			opvars=selenium.getText(Object);
		else
			opvars=selenium.getAttribute(Object);
		
		if(opvars.equalsIgnoreCase(expectedVal))
		{
			opvar=true;
			mess="Text is matched";
			
		}
		else
		{
			opvar=false;
			mess="Text is Not matched";
		}
		ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA[True]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
	//ret_mess="<KeywordOutput><Result>"+opvar+"</Result><Status>True</Status><Message>"+mess+"</Message></KeywordOutput>";
	}
	
	
	 catch (Exception e)
	 {
        setmessage(e.getMessage());
        opvar=false;
        mess=e.getMessage();
         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
  }
	return ret_mess;
	}
	
	public String Method_WaitForProperty(String Object,String PropertyName,String PropertyValue,String time)
	{
	 boolean opvar = false;
	 String opvars;
	 String mess,ret_mess;
	 Object=XmlString(Object);
	try
	{	
		Object=Object+"@"+PropertyName;
		int time1=Integer.parseInt(time);
		mess="Some Error Occoured";
		for(int i=0;i<time1;i++)
		{
			if(PropertyName.equalsIgnoreCase("innertext"))
				opvars=selenium.getText(Object);
			else
				opvars=selenium.getAttribute(Object);
		
			if(opvars.equalsIgnoreCase(PropertyValue))
			{
			  opvar=true;
			 
				break;
			  
			}  
			else
				waiting(1);
			 mess="Wait is complete";
		}
		ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA[True]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		//ret_mess="<KeywordOutput><Result>"+opvar+"</Result><Status>True</Status><Message>"+mess+"</Message></KeywordOutput>";
	}
	
	
	 catch (Exception e)
	 {
        setmessage(e.getMessage());
        opvar=false;
        mess=e.getMessage();
         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
  }
	return ret_mess;
	}
	
	
	
