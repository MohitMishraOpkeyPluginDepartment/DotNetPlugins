	public String Method_dragAndDropToObject(String object,String objectsec)
	{
	        		
				 boolean opvar;  
				 String mess,ret_mess;
				 object=XmlString(object);
				 objectsec=XmlString(objectsec);
				 
					try
					{
						//System.out.println("Method_ObjectClick"+object);
						selenium.dragAndDropToObject(object, objectsec);
					
						opvar=true;
						mess="Object is drag to another object";
