public String  Method_Login(String parameter,String username,String password,String browser,String url){
	boolean x = parameter.equalsIgnoreCase("yes");
	boolean opvar = false ;
	try{
	if(x==true)
	{
		// Method_CloseAllBrowsers()
	     //Method_ClosePage()
		host="localhost";
		port=5555;
	     Method_WebBrowserOpen(browser,url);
		 selenium.type("//input[@class='textfieldc'][@name='userId']",username);
		 selenium.type("//input[@class='textfieldc'][@name='password']",password);
		 selenium.click("//input[@id='Image2']");   
		 opvar = true;
	}
	else{
	boolean y =	selenium.isElementPresent("//image[contains(@href=welcome1.jpg)]");
	if(y==false){
		//Method_ClosePage()
      // Method_CloseAllBrowsers()
      // Method_WebBrowserOpen(browser,url)
		//selenium.type("//input[@class='textfieldc'][@name='userId']",username);
		//selenium.type("//input[@class='textfieldc'][@name='password']",password);
		 //selenium.click("//input[@id='Image2']");   
		 opvar = true;
		
	}
		
	}
	 String ret_mess = "<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA[true]]></Status><Message><![CDATA[VAlue is fetch]]></Message></KeywordOutput>";
	  	return ret_mess;
}catch(Exception e){
	
	//Method_CloseAllBrowsers()
   //Method_ClosePage()
   //Method_WebBrowserOpen(browser,url)
	selenium.type("//input[@class='textfieldc'][@name='userId']",username);
	selenium.type("//input[@class='textfieldc'][@name='password']",password);
	 selenium.click("//input[@id='Image2']");   
	 opvar = true;
	
}
	 String ret_mess = "<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA[true]]></Status><Message><![CDATA[VAlue is fetch]]></Message></KeywordOutput>";
	  	return ret_mess;
	
}	
	
	public static void waiting (int n)
{
        
        long t0, t1;

        t0 =  System.currentTimeMillis();

        do{
            t1 = System.currentTimeMillis();
        }
        while ((t1 - t0) < (n * 1000));
    }

