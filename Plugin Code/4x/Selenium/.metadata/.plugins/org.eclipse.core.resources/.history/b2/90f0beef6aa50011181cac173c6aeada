	 public boolean General_ExceptionsLoc(String newproperty)
	 {
		boolean eok;
		try{
			 //System.out.println("general");
		 eok=selenium.isElementPresent(newproperty);
		// System.out.println("genereok"+eok);
		}
		catch(Exception e)
		{
			// System.out.println("gencatch");
			setmessage(e.getMessage());
			eok=false;
		}
		return eok;
	 }
	 
	 public void setmessage(String message1 )
	 {
		// System.out.println("setmessage");
		 message=message1;
		// System.out.println(message);
	 }
public String XmlString(String xmlRecords)
{
	String property1=null,value1=null,regexp1 = null,newproperty1[],type,mainstr = null,type1[];
	int k=0;

//	System.out.println(xmlRecords);
/*	String xmlRecords1=
			"<Opkey:ObjectArguments>"+
    "<Opkey:ObjectArgument argumentName='TextField'>"+
      "<Opkey:Object class='HTML PAGE' htmlType='HTML PAGE' tag=' '>"+
        "<Opkey:Properties>"+
         " <Opkey:Property name='url' regularExpression='0'>http://www.google.co.in/</Opkey:Property>"+
         " <Opkey:Property name='title' regularExpression='0'>Google</Opkey:Property>"+
         " <Opkey:Property name='type' regularExpression='0'>PAGE</Opkey:Property>"+
       " </Opkey:Properties>"+
        "<Opkey:ChildObject>"+
         " <Opkey:Object class='Text Field' htmlType='Input Text' tag=' '>"+
           " <Opkey:Properties>"+
             " <Opkey:Property name='name' regularExpression='0'>q</Opkey:Property>"+
              "<Opkey:Property name='className' regularExpression='0'>lst</Opkey:Property>"+
              "<Opkey:Property name='type' regularExpression='1'>text</Opkey:Property>"+
            "</Opkey:Properties>"+
          "</Opkey:Object>"+
       " </Opkey:ChildObject>"+
      "</Opkey:Object>"+
   " </Opkey:ObjectArgument>"+
  "</Opkey:ObjectArguments>";
*/
	newproperty1=new String[6];
    
    	    try {
    	        DocumentBuilderFactory dbf =
    	            DocumentBuilderFactory.newInstance();
    	        DocumentBuilder db = dbf.newDocumentBuilder();
    	        InputSource is = new InputSource();
    	        is.setCharacterStream(new StringReader(xmlRecords));

    	        Document doc = db.parse(is);
    	      
    	        NodeList nodes = doc.getElementsByTagName("Opkey:ChildObject");
    	        Node cnode = nodes.item(0);
    	        //System.out.println(cnode.getChildNodes().getLength());
    		    Element line2=(Element)cnode.getChildNodes().item(1);
    		   // System.out.println(line2.getAttribute("htmlType"));
    		    type=line2.getAttribute("tag");
    		   // type1=type.split(" ");
    		    //type=type1[0];
    		    Element element12 = (Element)nodes.item(0);
    		    NodeList name1 = element12.getElementsByTagName("Opkey:Property");
    		   // System.out.println("childobj "+name1.getLength());
    		    for(int i=0;i<name1.getLength();i++)
    		    {
    		    	 Element line1 = (Element) name1.item(i);
    		    	 property1= line1.getAttribute("name");
    		    		//	 System.out.println(line1.getAttribute("name"));
    		    	// System.out.println(line1.getAttribute("regularExpression"));
    		    	 regexp1=line1.getAttribute("regularExpression");
    		    	// System.out.println(getCharacterDataFromElement(line1));
    		    	 value1=getCharacterDataFromElement(line1);
    		    
    	        
    	        
    	       /* for (int i = 0; i < nodes.getLength(); i++) {
    	           Element element = (Element) nodes.item(i);

    	           NodeList name = element.getElementsByTagName("Property");
    	           Element line = (Element) name.item(0);
    	           property1=getCharacterDataFromElement(line);
    	          // System.out.println("<Property>: " + property1);
    	           
    	           NodeList title = element.getElementsByTagName("Value");
    	           line = (Element) title.item(0);
    	           value1=getCharacterDataFromElement(line);
    	           System.out.println("<Value>: " + value1);
    	           
    	           NodeList reflag = element.getElementsByTagName("Regflag");
    	           line = (Element) reflag.item(0);
    	           regexp1=getCharacterDataFromElement(line);
    	           System.out.println("<Reflag>: " + regexp1);
    	           //System.out.println("Regflag: " + getCharacterDataFromElement(line));
    	         */  
    		           if(regexp1.compareTo("1")==0)
   				        	value1="*"+value1+"*";
    	               if(i==0)
    	                    {
    	                      if(property1.compareToIgnoreCase("index")==0) 
    	                    	  newproperty1[i]="["+value1+"]";
    	                      else if((property1.compareToIgnoreCase("text")==0) ||(property1.compareToIgnoreCase("innertext")==0) )
    	                    	  newproperty1[i]="[contains(text(),\'"+value1+"\')]";	
    	                      else if((property1.compareToIgnoreCase("xpath")==0))
    	                    	  mainstr=value1;
    	        	   //newproperty1[i]=value1;
    	                      else if((property1.compareToIgnoreCase("className")==0))
    	                    	  	newproperty1[i]="[@class="+"'"+value1+"']";
    	                      else  
    	                    	  newproperty1[i]="[@"+property1+"="+"'"+value1+"']";	
    	                    // System.out.println(newproperty1[i]);
    	                     if(i==0 && (property1.compareToIgnoreCase("xpath")!=0) )
    	        	              mainstr="//"+type+newproperty1[i];
    	         
    	                    }
    	               if(i>0)
    	               {
    	            	   if(property1.compareToIgnoreCase("index")==0) 
    	            		   newproperty1[i]="["+value1+"]";
    	            	   else if((property1.compareToIgnoreCase("text")==0) ||(property1.compareToIgnoreCase("innertext")==0) )
    	            		   newproperty1[i]="[contains(text(),\'"+value1+"\')]";	
    	            	   else if((property1.compareToIgnoreCase("xpath")==0))
    	            		   mainstr=value1;
    	            	   else if((property1.compareToIgnoreCase("className")==0))
    	            		   newproperty1[i]="[@class='"+value1+"']";
       	        	   //newproperty1[i]=value1;
    	            	   else
    	            		   newproperty1[i]="[@"+property1+"='"+value1+"']";	
    	            	  // System.out.println(newproperty1[i]);
    	            	   if(i==0 && (property1.compareToIgnoreCase("xpath")!=0) )
    	            		   mainstr="//"+type+newproperty1[i];
    	               }
    	           k++;
    	          // System.out.println("k"+k);
    	        }
    	        for(int j=1;j<k;j++)
    	        	mainstr=mainstr+newproperty1[j];
    	        System.out.println("xml"+mainstr);
    	    }
    	    catch (Exception e) {
    	    	//setmessage(e.getMessage());
    	        e.printStackTrace();
    	        mainstr=e.getMessage();
    	    }
			return mainstr;
    	   
}
	 
public static String getCharacterDataFromElement(Element e) {
    Node child = e.getFirstChild();
    if (child instanceof CharacterData) {
       CharacterData cd = (CharacterData) child;
       return cd.getData();
    }
    return "?";
  }
//******** Method to return minimum value of given digit lengths *****
	public static String Method_ReturnMinValueGivenLength(String lengths)
	{
		int Len= Integer.parseInt(lengths);
		String ret_mess;
		int Len1= Len-2;
		String data="1";
		try
		{
		  for(int i=0;i<=Len1;i++)
		    {
			data = data+"0";
		    }
	      String mess= "Minimum Value generated";
        ret_mess="<KeywordOutput><Result><![CDATA["+data+"]]></Result><Status><![CDATA["+true+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
    	}
		catch (Exception e)
		  {
		   e.printStackTrace();
		   ret_mess= e.getMessage();
		  }
		return ret_mess;
	}
	
	
//******** Method to return maximum value of given digit lengths *****
		public static String Method_ReturnMaxValueGivenLength(String lengths)
		{
			int Len= Integer.parseInt(lengths);
			String ret_mess;
			int Len1= Len-1;
			String data="";
			try
			{
			  for(int i=0;i<=Len1;i++)
			    {
				data = data+"9";
			    }
		      String mess= "Maximum Value generated";
	          ret_mess="<KeywordOutput><Result><![CDATA["+data+"]]></Result><Status><![CDATA["+true+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
			}
			catch (Exception e)
			  {
			   e.printStackTrace();
			   ret_mess= e.getMessage();
			  }
			return ret_mess;
		}
		
		

//******** Method to return message *****

public static String Method_ReportMessage(String status,String text)
		{
			String opvar;
			String ret_mess;
			if (status.equalsIgnoreCase("false")|| status.equalsIgnoreCase("fail") || status.equalsIgnoreCase("failed"))
			{
			 opvar="Report Message False";
		     ret_mess="<KeywordOutput><Result><![CDATA["+status+"]]></Result><Status><![CDATA["+true+"]]></Status><Message><![CDATA["+text+"]]></Message></KeywordOutput>"; 
		    }
			else if(status.equalsIgnoreCase("true")|| status.equalsIgnoreCase("pass")||status.equalsIgnoreCase("passed"))
			{
			opvar="Report Message True";
			ret_mess="<KeywordOutput><Result><![CDATA["+status+"]]></Result><Status><![CDATA["+true+"]]></Status><Message><![CDATA["+text+"]]></Message></KeywordOutput>"; 
			}
          else 
          { 
          opvar="Status is not properly defined";
          ret_mess="<KeywordOutput><Result><![CDATA[Invalid Status]]></Result><Status><![CDATA["+true+"]]></Status><Message><![CDATA["+text+"]]></Message></KeywordOutput>"; 
          }
          return  ret_mess;
		 }

		

