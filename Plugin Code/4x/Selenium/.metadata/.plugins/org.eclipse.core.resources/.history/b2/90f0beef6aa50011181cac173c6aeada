	public String Method_getFullTableText(String object)
	{
	        		
				 boolean opvar;  
				 String mess,ret_mess;
				 String no,row,result ="",nocol,newobj,newresult="";
				 int noint = 0,colno=0;
				 object=XmlString(object);
					try
					{
						//System.out.println("Method_ObjectClick"+object);
						mess="object is not found";
						if(selenium.isElementPresent(object))
						{
							//no=Method_GetRowCount(object);
						//	nocol=Method_GetColCount(object);
							//noint=Integer.parseInt(no);
							//colno=Integer.parseInt(nocol);
							for(int i=1;i<=1000;i++)
							{	     
								newobj=object+"/tbody/tr["+i+"]";  
								if(selenium.isElementPresent(newobj))
								{
									for(int j=1;j<=500;j++)
									{
										//newobj=object+"/tbody/tr["+i+"]";
										
											newobj=object+"/tbody/tr["+i+"]/td["+j+"]";
											System.out.println(newobj);
											if(selenium.isElementPresent(newobj))
											{
												result=selenium.getText(newobj);
												//System.out.println(result);
												if(result.isEmpty()==false)	
													newresult=newresult+result+";";
												System.out.println(newresult);
											}
											else
											{
												break;
											}
									}		
									
								}		
								else
								{
								 break;
								}
								
							}  
							opvar=true;
							mess="data is retrive";
						}
						else
						{
							mess="object is not found";
							opvar=false;
						}
						ret_mess="<KeywordOutput><Result><![CDATA["+newresult+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						//ret_mess="<KeywordOutput><Result>"+newresult+"</Result><Status>"+opvar+"</Status><Message>"+mess+"</Message></KeywordOutput>";
					}
					 catch (Exception e)
					 {
				        setmessage(e.getMessage());
				        opvar=false;
				        mess=e.getMessage();
				         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";

				    }
					return ret_mess;
				
	}
	
	public String Method_TableGetTextColumn(String object,String rowid,String text)
	{
	        		
				 boolean opvar;  
				 String mess,ret_mess;
				 String no,row,result,newobj;
				 int noint = 0;
				 object=XmlString(object);
					try
					{
						//System.out.println("Method_ObjectClick"+object);
						mess="object is not found";
						if(selenium.isElementPresent(object))
						{
							//no=Method_GetColCount(object);
							//noint=Integer.parseInt(no);
							for(int i=1;i<=500;i++)
							{	     
										newobj=object+"/tbody/tr["+rowid+"]/td["+i+"]";
										System.out.println(newobj);
										if(selenium.isElementPresent(newobj))
										{
											result= selenium.getText(newobj);
											System.out.println(result);
											if(result.equalsIgnoreCase(text))
											{
												noint=i;
												mess="Data is matched";
												break;
											}
											else
												mess="Data is not matched";
										}
										else
										{
											break;
										}
							}	  
							opvar=true;
							
								
						}
						else
						{
							mess="object is not found";
							opvar=false;
						}
						ret_mess="<KeywordOutput><Result><![CDATA["+noint+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						//ret_mess="<KeywordOutput><Result>"+noint+"</Result><Status>"+opvar+"</Status><Message>"+mess+"</Message></KeywordOutput>";
					}
					 catch (Exception e)
					 {
				        setmessage(e.getMessage());
				        opvar=false;
				        mess=e.getMessage();
				         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						  // e.printStackTrace();
				    }
					return ret_mess;
				
	}

	
	public String Method_GetColCount(String object)
	{
	
		 String nos;
		 Number no;
		
			object=object + "/tbody/td";
			no=selenium.getXpathCount(object);
			nos=no.toString();
			System.out.println("colcount"+nos);
	    return nos;
	}
	
	public String Method_GetRowCount(String object)
	{

		 String nos;
		 Number no;
		
			object=object + "/tbody/td";
			System.out.println("rowcount"+object);
			//object=object + "/tbody/tr";
			no=selenium.getXpathCount(object);
			nos=no.toString();
			System.out.println("rowcount"+nos);
			object=object + "/tbody/tr";
			no=selenium.getXpathCount(object);
			System.out.println("rowcount"+object);
			nos=no.toString();
			System.out.println("rowcount"+nos);
			object=object + "/tbody/";
			no=selenium.getXpathCount(object);
			System.out.println("rowcount"+object);
			nos=no.toString();
			System.out.println("rowcount"+nos);
	    return nos;
	}
	public String Method_doubleClickAndWait(String object,String time)
	{
	        		
				 boolean opvar;  
				 String mess,ret_mess;
				 object=XmlString(object);
					try
					{
						
						selenium.doubleClick(object);
						
						//setmessage("Object is Double Clicked");
						opvar=true;
						mess="Object is Double Clicked";
					

						int time1=Integer.parseInt(time);
						waiting(time1);
						 ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
					}
					 catch (Exception e)
					 {
				        setmessage(e.getMessage());
				        opvar=false;
				        mess=e.getMessage();
				         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						  // e.printStackTrace();
				    }
					return ret_mess;
				
	}
	
