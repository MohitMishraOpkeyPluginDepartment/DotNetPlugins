	public String Method_verifyCheckBoxStatus(String newproperty,String value)
	{
		boolean opvar,opvar1; 
		String ret_mess,mess;
		 newproperty=XmlString(newproperty);
		try
		{
			
			opvar=selenium.isChecked(newproperty);
			//mess="CheckBox is unchecked";
			if(opvar==true)
			{	if(value.equalsIgnoreCase("on"))
						{
						mess="Matched";
						opvar1=true;
						}
				else
				{
					mess="Not Matched";
					opvar1=false;	
				}
			}
			else
			{
				if(value.equalsIgnoreCase("on"))
						{
					mess="Not Matched";
					opvar1=false;
						}
				else
				{
					mess="Matched";
					opvar1=true;
				}
			}
			ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA[True]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
			//ret_mess="<KeywordOutput><Result>"+opvar1+"</Result><Status>True</Status><Message>"+mess+"</Message></KeywordOutput>";
			
		}
		 catch (Exception e)
		 {
	       
	        opvar=false;
	        mess=e.getMessage();
	         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		  }
		return ret_mess;
	}
