	public String Method_getSelectedRadio(String object)
	{
	        		
				 boolean opvar,opvar1;  
				 String mess,ret_mess,newobj;
				 object=XmlString(object);
					try
					{
					int i=1;
					
						while(true)
						{
						opvar1=selenium.isChecked(object);
						if(opvar1==true)
							break;
						else
							newobj=object+"["+i+"]";
						i++;
						if(i>100)
							break;
						}
						//setmessage("Object is Double Clicked");
						opvar=true;
						mess=" Cell Clicked";
						ret_mess="<KeywordOutput><Result><![CDATA["+i+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						//ret_mess="<KeywordOutput><Result>"+i+"</Result><Status>"+opvar+"</Status><Message>"+mess+"</Message></KeywordOutput>";
					}
					 catch (Exception e)
					 {
				        setmessage(e.getMessage());
				        opvar=false;
				        mess=e.getMessage();
				         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						  // e.printStackTrace();
				    }
