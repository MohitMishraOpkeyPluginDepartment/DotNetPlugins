	public String Method_TableGetTextRow(String object,String Colid,String text)
	{
	        		
				 boolean opvar;  
				 String mess,ret_mess;
				 String no,row,result,newobj;
				 int noint = 0;
				 object=XmlString(object);
					try
					{
						//System.out.println("Method_ObjectClick"+object);
						mess="object is not found";
						if(selenium.isElementPresent(object))
						{
							 System.out.println("inside first result");
							//no=Method_GetRowCount(object);
							//noint=Integer.parseInt(no);
							for(int i=1;i<=5000;i++)
							{	     
								    
									  newobj=object +"/tbody/tr["+i+"]";
									  System.out.println("result123"+newobj);
									  if(selenium.isElementPresent(newobj)==true)
									  {	  
								      newobj=object +"/tbody/tr["+i+"]/td["+Colid+"]";
								      System.out.println("result"+newobj);
									  result= selenium.getText(newobj);
									  System.out.println("result"+result);
									  if(result.equalsIgnoreCase(text))
									  {
										  noint=i;
										  mess="Data is matched";
										  break;
									  }
									  else
										  mess="Data is not matched";
									  }
									  else
									  {
										  break;
									  }
							}	  
							opvar=true;
							
								
						}
						else
						{
							mess="object is not found";
							opvar=false;
						}
						 ret_mess="<KeywordOutput><Result><![CDATA["+noint+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						//ret_mess="<KeywordOutput><Result>"+noint+"</Result><Status>"+opvar+"</Status><Message>"+mess+"</Message></KeywordOutput>";
					}
					 catch (Exception e)
					 {
				        setmessage(e.getMessage());
				        opvar=false;
				        mess=e.getMessage();
				         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						  // e.printStackTrace();
				    }
					return ret_mess;
				
	}
	
	public String Method_verifyTextInTable(String object,String Rowid,String Colid,String text)
	{
	        		
				 boolean opvar;  
				 String mess,ret_mess;
				 String result;
	
				 object=XmlString(object);
					try
					{
						object=object+"/tbody/tr["+Rowid+"]/td["+Colid+"]";
						result=selenium.getText(object);
						if(result.equalsIgnoreCase(text))
						{
							mess="Data is matched";
							opvar=true;
						}
						else
							{
							mess="Data is not matched";
							opvar=false;
							}
						ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA[True]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						//ret_mess="<KeywordOutput><Result>"+opvar+"</Result><Status>True</Status><Message>"+mess+"</Message></KeywordOutput>";
					}
					 catch (Exception e)
					 {
				        setmessage(e.getMessage());
				        opvar=false;
				        mess=e.getMessage();
				         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						  // e.printStackTrace();
				    }
