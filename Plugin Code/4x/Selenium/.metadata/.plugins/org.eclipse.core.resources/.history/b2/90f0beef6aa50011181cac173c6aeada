		public String Method_TopMenuClick(String name)
		{
		 	boolean opvar;
		    String ret_mess;
		    String mess= " ' " + name + "' Top Menu is Clicked";
		    try
		    {
			 if(name.contains("("))
		      {
		       name=name.split("(")[0];
		       name = name.trim();
		       String str = "//li/a/span[contains(text(),'"+name+"')]";
		       selenium.click(str);
		       opvar=true;
		       ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+true+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
		      }		    
		    else if (name.contains("/"))
		      {  
		       name=name.split("/")[0];
		      }
		      name= name.trim();
		      String str = "//li/a/span[contains(text(),'" + name + "')]";
		      selenium.click(str);
		      opvar=true;
		      ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+true+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>"; 
		    
		    }
		    catch (Exception e)
		    {
				   e.printStackTrace();
				   ret_mess= e.getMessage();
				  }
		
		    return ret_mess;
	     } 
