	}
	
	public String Method_setChildTableCellText(String object,String propertyName,String propertyValue,String Rolid,String Colid,String text,String type)
	{
	        		
				 boolean opvar;  
				 String mess,ret_mess;
				 String execstring;
				 object=XmlString(object);
					try
					{
						//System.out.println("Method_ObjectClick"+object);
						mess="object is not found";
				//		"//input[contains(@"+propertyName+",'"+propertyValue+"')]"
						  if (propertyName.equalsIgnoreCase("text")|| propertyName.equalsIgnoreCase("innertext")) 	
							  execstring = "table[contains(text(),\'"+propertyValue+"\')]";
				
						  else
							  execstring= "table[@"+propertyName+"='"+propertyValue+"']";
						  if(type.equalsIgnoreCase("_blank"))
							  object=object+"/descendant::"+execstring+"/tbody/tr["+Rolid+"]/td["+Colid+"]";	
						  else
							  object=object+"/descendant::"+execstring+"/tbody/tr["+Rolid+"]/td["+Colid+"]/"+type;
						 // object=object+"/descendant::"+execstring+"/tbody/tr["+Rolid+"]/td["+Colid+"]/input";					//div[contains(@class, 'x-combo-list')]/descendant::div[contains(@class, 'x-combo-list-item')][text()='AUTO']"
						selenium.type(object, text);
						System.out.println(object);
						mess="Text is Set"; 
						opvar=true;
						 ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
					}
					 catch (Exception e)
					 {
				        setmessage(e.getMessage());
				        opvar=false;
				        mess=e.getMessage();
				         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
						  // e.printStackTrace();
				    }
					return ret_mess;
				
	}
	
	
	
