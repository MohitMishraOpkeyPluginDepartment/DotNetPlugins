	public String Method_VerifyListItem(String Object,String Value)
	{
		boolean opvar;
		String opvars[],opvarst;
		String mess,ret_mess;
		Object=XmlString(Object);
		try
		{
		
		 opvars=selenium.getSelectedLabels(Object);
		//setmessage("Item is selected");
		
		
		opvar=false;
		for(int i=0;i<opvars.length;i++)
		{
			if(opvars[i].equalsIgnoreCase(Value))
				{
				opvar=true;
				break;
				}
			else
			 opvar=false;
				
			
		}
		if(opvar==true)
			mess="item is present";
		else
			mess="item is not present";
		ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA[True]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
	    //ret_mess="<KeywordOutput><Result>"+opvar+"</Result><Status>True</Status><Message>"+mess+"</Message></KeywordOutput>";
		}
		 catch (Exception e)
		 {
	        setmessage(e.getMessage());
	        opvar=false;
	        mess=e.getMessage();
	         ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
	  }
		return ret_mess;
	}
	
	
	public String Method_verifyObjectVisible(String newproperty)
	{
		boolean opvar;  
		String ret_mess,mess;
		 newproperty=XmlString(newproperty);
		try
		{
			//System.out.println("Method_SelectRadio"+newproperty);
			
			opvar=selenium.isVisible(newproperty);
			//selenium.click(newproperty);
			setmessage("Object is Visible");
			if(opvar==true)
		     	mess="Object is Visible";
			else
				mess="Object is not Visible";
			ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA[True]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
			//ret_mess="<KeywordOutput><Result>"+opvar+"</Result><Status>True</Status><Message>"+mess+"</Message></KeywordOutput>";
		}
		 catch (Exception e)
		 {
	        setmessage(e.getMessage());
	        opvar=false;
	        mess=e.getMessage();
	        ret_mess="<KeywordOutput><Result><![CDATA["+opvar+"]]></Result><Status><![CDATA["+opvar+"]]></Status><Message><![CDATA["+mess+"]]></Message></KeywordOutput>";
	        //ret_mess="<KeywordOutput><Result>"+opvar+"</Result><Status>True</Status><Message>"+mess+"</Message></KeywordOutput>";
		  }
		return ret_mess;
