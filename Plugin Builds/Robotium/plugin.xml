    <Arguments>-jar robotiumplugin-0.10.jar $SharedDBPath$ $ScreenshotsDirectory$ $SessionSnapshotFrequency$</Arguments>
  </Invocation>
  <CanTakeSnapshot>1</CanTakeSnapshot>
<LicenseIdentifier></LicenseIdentifier>
</plugin>
